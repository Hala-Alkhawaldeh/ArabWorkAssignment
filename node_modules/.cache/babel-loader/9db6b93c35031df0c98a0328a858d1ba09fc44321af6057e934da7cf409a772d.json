{"ast":null,"code":"import axios from \"axios\";\nimport doctorCard from \"./doctorCard.vue\";\nimport Gmap from './gmap.vue';\nexport default {\n  components: {\n    doctorCard,\n    Gmap\n  },\n  data() {\n    return {\n      users: [],\n      currentPage: 1,\n      usersPerPage: 5\n    };\n  },\n  methods: {\n    async fetchUsers() {\n      try {\n        const response = await axios.get(\"https://randomuser.me/api/\", {\n          params: {\n            results: this.usersPerPage,\n            page: this.currentPage\n          }\n        });\n        this.users = response.data.results;\n        console.log(this.users);\n      } catch (error) {\n        console.error(error);\n      }\n    },\n    nextPage() {\n      this.currentPage++;\n      this.fetchUsers();\n    }\n  },\n  computed: {},\n  created() {\n    this.fetchUsers();\n  }\n};","map":{"version":3,"names":["axios","doctorCard","Gmap","components","data","users","currentPage","usersPerPage","methods","fetchUsers","response","get","params","results","page","console","log","error","nextPage","computed","created"],"sources":["src/components/HelloWorld.vue"],"sourcesContent":["<template>\n  <div class=\"flex\">\n    <div class=\"user-list\">\n      <div class=\"user-cards\">\n        <div class=\"search-result-blk my-10 container\">\n      <div class=\"grid grid-cols-1 gap-y-6 gap-x-14\" >\n        <div\n          v-for=\"user in users\"\n          :key=\"user.login.uuid\"\n          @click=\"showUserOnMap(user)\"\n        >\n          <doctor-card :user=\"user\"></doctor-card>\n          <!-- Display user card data -->\n        </div>\n      </div>\n        </div>\n        </div>\n      <button @click=\"nextPage\">Next</button>\n    </div>\n    <div>\n    <div>\n      <h2>Vue Js Search and Add Marker</h2>\n      <label>\n        <gmap-autocomplete @place_changed=\"initMarker\"></gmap-autocomplete>\n        <button @click=\"addLocationMarker\">Add</button>\n      </label>\n      <br/>\n \n    </div>\n    <br>\n    <gmap-map\n        :zoom=\"14\"    \n        :center=\"center\"\n        style=\"width:100%;  height: 600px;\"\n      >\n      <gmap-marker\n        :key=\"index\"\n        v-for=\"(m, index) in locationMarkers\"\n        :position=\"m.position\"\n        @click=\"center=m.position\"\n      ></gmap-marker>\n    </gmap-map>\n  </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport doctorCard from \"./doctorCard.vue\";\nimport Gmap from './gmap.vue';\n\n\nexport default {\n  components: { doctorCard, Gmap },\n  data() {\n    return {\n      users: [],\n      currentPage: 1,\n      usersPerPage: 5,\n    };\n  },\n  methods: {\n    async fetchUsers() {\n      try {\n        const response = await axios.get(\"https://randomuser.me/api/\", {\n          params: {\n            results: this.usersPerPage,\n            page: this.currentPage,\n          },\n        });\n\n        this.users =response.data.results ;\n        console.log(this.users);\n      } catch (error) {\n        console.error(error);\n      }\n    },\n \n    nextPage() {\n      this.currentPage++;\n      this.fetchUsers();\n    },\n  },\n  computed: {\n \n  },\n  created() {\n    this.fetchUsers();\n  },\n};\n</script>"],"mappings":"AA+CA,OAAAA,KAAA;AACA,OAAAC,UAAA;AACA,OAAAC,IAAA;AAGA;EACAC,UAAA;IAAAF,UAAA;IAAAC;EAAA;EACAE,KAAA;IACA;MACAC,KAAA;MACAC,WAAA;MACAC,YAAA;IACA;EACA;EACAC,OAAA;IACA,MAAAC,WAAA;MACA;QACA,MAAAC,QAAA,SAAAV,KAAA,CAAAW,GAAA;UACAC,MAAA;YACAC,OAAA,OAAAN,YAAA;YACAO,IAAA,OAAAR;UACA;QACA;QAEA,KAAAD,KAAA,GAAAK,QAAA,CAAAN,IAAA,CAAAS,OAAA;QACAE,OAAA,CAAAC,GAAA,MAAAX,KAAA;MACA,SAAAY,KAAA;QACAF,OAAA,CAAAE,KAAA,CAAAA,KAAA;MACA;IACA;IAEAC,SAAA;MACA,KAAAZ,WAAA;MACA,KAAAG,UAAA;IACA;EACA;EACAU,QAAA,GAEA;EACAC,QAAA;IACA,KAAAX,UAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}