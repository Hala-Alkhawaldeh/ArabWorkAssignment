>\nimport axios from \"axios\";\nimport doctorCard from \"./doctorCard.vue\";\nimport Maps from './maps.vue';\n\n\nexport default {\n  components: { doctorCard, Maps },\n  data() {\n    return {\n      users: [],\n      currentPage: 1,\n      usersPerPage: 5,\n    };\n  },\n  methods: {\n    async fetchUsers() {\n      try {\n        const response = await axios.get(\"https://randomuser.me/api/\", {\n          params: {\n            results: this.usersPerPage,\n            page: this.currentPage,\n          },\n        });\n\n        this.users =response.data.results ;\n        console.log(this.users);\n      } catch (error) {\n        console.error(error);\n      }\n    },\n \n    nextPage() {\n      this.currentPage++;\n      this.fetchUsers();\n    },\n    showUserOnMap(user) {\n      // Call the child function using a ref\n      this.$refs.childComponentRef.initMarker(user.location.coordinates);\n    }\n  },\n  computed: {\n \n  },\n  created() {\n    this.fetchUsers();\n  },\n};\n</script>"],"mappings":"AAqBA,OAAAA,KAAA;AACA,OAAAC,UAAA;AACA,OAAAC,IAAA;AAGA;EACAC,UAAA;IAAAF,UAAA;IAAAC;EAAA;EACAE,KAAA;IACA;MACAC,KAAA;MACAC,WAAA;MACAC,YAAA;IACA;EACA;EACAC,OAAA;IACA,MAAAC,WAAA;MACA;QACA,MAAAC,QAAA,SAAAV,KAAA,CAAAW,GAAA;UACAC,MAAA;YACAC,OAAA,OAAAN,YAAA;YACAO,IAAA,OAAAR;UACA;QACA;QAEA,KAAAD,KAAA,GAAAK,QAAA,CAAAN,IAAA,CAAAS,OAAA;QACAE,OAAA,CAAAC,GAAA,MAAAX,KAAA;MACA,SAAAY,KAAA;QACAF,OAAA,CAAAE,KAAA,CAAAA,KAAA;MACA;IACA;IAEAC,SAAA;MACA,KAAAZ,WAAA;MACA,KAAAG,UAAA;IACA;IACAU,cAAAC,IAAA;MACA;MACA,KAAAC,KAAA,CAAAC,iBAAA,CAAAC,UAAA,CAAAH,IAAA,CAAAI,QAAA,CAAAC,WAAA;IACA;EACA;EACAC,QAAA,GAEA;EACAC,QAAA;IACA,KAAAlB,UAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}